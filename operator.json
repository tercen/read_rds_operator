{
  "name": "RDS",
  "description": "Imports an R-Data object (.rds).",
  "tags": [
    "import"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/read_rds_operator"
  ],
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Document ID",
            "type": "string",
            "description": "Document ID(s) of RDS file(s).",
            "ontologyMapping": "document",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "OperatorJoinSpec",
        "description": "Dynamic columns from RDS file content",
        "joinOperators": [
          {
            "kind": "JoinOperator",
            "joinType": "left",
            "leftPair": {
              "kind": "ColumnPair",
              "lColumns": [
                "Document ID"
              ],
              "rColumns": [
                "Document ID"
              ]
            },
            "rightRelation": {
              "kind": "TableRelation",
              "description": "Columns dynamically loaded from RDS file",
              "attributes": []
            }
          }
        ]
      }
    ]
  }
}